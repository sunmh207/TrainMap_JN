<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN"   "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	
	<field name="lh.ch">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>车号不能为空</message>
		</field-validator>
	</field>
	<field name="lh.ch">
		<field-validator type="stringlength">
			<param name="minLength">2</param>
			<param name="maxLength">16</param>
			<message>车号长度必须在 ${minLength} 到 ${maxLength}之间</message>
		</field-validator>
	</field>
	<field name="lh.mainPart">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">100</param>
			<message>主部件长度必须在 ${minLength} 到 ${maxLength}之间</message>
		</field-validator>
	</field>
	<field name="lh.partName">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">20</param>
			<message>主部件长度必须在 ${minLength} 到 ${maxLength}之间</message>
		</field-validator>
	</field>
	<field name="lh.symptom">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>故障现象不能为空</message>
		</field-validator>
	</field>
	<field name="lh.symptom">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">1000</param>
			<message>故障现象必须在 ${minLength} 到 ${maxLength}之间</message>
		</field-validator>
	</field>
</validators>