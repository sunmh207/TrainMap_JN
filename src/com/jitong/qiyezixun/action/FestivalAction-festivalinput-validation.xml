<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN"   "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="festival.festivalName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>节日名称不能为空</message>
		</field-validator>
	</field>
	<field name="festival.festivalDate">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>日期不能为空</message>
		</field-validator>
	</field>
	<field name="festival.SMSContent">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>内容不能为空</message>
		</field-validator>
	</field>
</validators>